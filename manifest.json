{
  "manifest_version": 3,
  "name": "Backlog Theme Icon Changer",
  "version": "1.0",
  "description": "Changes the tab icon based on the Backlog theme.",
  "content_scripts": [
    {
      "matches": [
        "https://*.backlog.jp/*",
        "https://*.backlog.com/*"
      ],
      "js": [
        "dist/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/*.png"
      ],
      "matches": [
        "https://*.backlog.jp/*",
        "https://*.backlog.com/*"
      ]
    }
  ]
}